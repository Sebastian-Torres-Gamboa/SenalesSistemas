        "st.set_page_config(\n",
        "    page_title=\"Explorador Interactivo SyS - Parcial 2\",  # TÃ­tulo que aparece en la pestaÃ±a del navegador\n",
        "    page_icon=\"ğŸ“˜\",  # Icono de la pestaÃ±a del navegador\n",
        ")\n",
        "\n",
        "# Divide la pÃ¡gina en tres columnas con proporciones 1:4:1 para centrar el contenido\n",
        "col_a, col_b, col_c = st.columns([1, 5, 1])  # col_b serÃ¡ la columna central principal\n",
        "\n",
        "# SecciÃ³n de contenido centrado en la columna del medio\n",
