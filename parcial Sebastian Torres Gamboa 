    {
      "cell_type": "code",
      "source": [
        "%%writefile 2_Modulador_SSB_FFT.py\n",
        "# ImportaciÃ³n de bibliotecas necesarias\n",
        "import streamlit as st\n",
        "import numpy as np\n",
        "from scipy.fft import fft, ifft, fftshift, ifftshift, fftfreq\n",
        "from scipy.io import wavfile\n",
        "import matplotlib.pyplot as plt\n",
        "import yt_dlp\n",
        "import os\n",
        "import subprocess\n",
        "\n",
        "# ConfiguraciÃ³n inicial de la pÃ¡gina web del dashboard\n",
        "st.set_page_config(\n",
        "    page_title=\"SimulaciÃ³n SSB-AM vÃ­a FFT\",  # TÃ­tulo de la pestaÃ±a\n",
        "    page_icon=\"ğŸšï¸\",                         # Ãcono de la pestaÃ±a\n",
        "    layout=\"wide\",                          # Layout ancho\n",
        ")\n",
        "\n",
        "# TÃ­tulo principal de la aplicaciÃ³n\n",
        "st.title(\"ğŸšï¸ Plataforma de SSB-AM por Espectro en Frecuencia\")\n",
        "\n",
        "# DescripciÃ³n breve de la funcionalidad\n",
        "st.write(\n",
        "    \"Este entorno permite visualizar la modulaciÃ³n y recuperaciÃ³n de seÃ±ales SSB mediante \"\n",
        "    \"procesamiento espectral con FFT.\"\n",
        ")\n",
        "\n",
        "# Mensaje informativo sobre requisitos para usar YouTube\n",
        "st.info(\"Para usar enlaces de YouTube asegÃºrate de tener `yt-dlp` y `ffmpeg` instalados correctamente.\", icon=\"ğŸ’¡\")\n",
        "\n",
        "# -----------------------------------------------\n",
        "# DEFINICIÃ“N DE FUNCIONES AUXILIARES\n",
        "# -----------------------------------------------\n",
        "\n",
        "@st.cache_data\n",
        "def obtener_audio_desde_youtube(url, duracion=5, inicio=0):\n",
        "    \"\"\"\n",
        "    Descarga un segmento de audio desde YouTube y lo convierte a WAV mono para su anÃ¡lisis.\n",
        "    Utiliza cachÃ© para evitar descargas repetidas.\n",
        "    \"\"\"\n",
        "    archivo_temp = 'temp_audio.wav'           # Nombre del archivo descargado\n",
        "    archivo_recorte = 'cropped_audio.wav'     # Nombre del archivo ya recortado\n",
        "\n",
        "    try:\n",
        "        # ConfiguraciÃ³n para yt_dlp\n",
        "        ydl_opts = {\n",
        "            'format': 'bestaudio/best',\n",
        "            'postprocessors': [{'key': 'FFmpegExtractAudio', 'preferredcodec': 'wav'}],\n",
        "            'outtmpl': 'temp_audio',\n",
        "            'quiet': True,\n",
        "        }\n",
        "\n",
        "        # Elimina archivos anteriores si existen\n",
        "        if os.path.exists(archivo_temp):\n",
        "            os.remove(archivo_temp)\n",
        "        if os.path.exists(archivo_recorte):\n",
        "            os.remove(archivo_recorte)\n",
        "\n",
        "        # Descarga el audio desde la URL proporcionada\n",
        "        with yt_dlp.YoutubeDL(ydl_opts) as ydl:\n",
        "            ydl.download([url])\n",
        "\n",
        "        # Recorta el audio descargado usando ffmpeg\n",
        "        comando = [\n",
        "            'ffmpeg', '-i', archivo_temp, '-ss', str(inicio),\n",
        "            '-t', str(duracion), '-acodec', 'pcm_s16le',\n",
        "            '-ar', '44100', '-ac', '1', archivo_recorte, '-y'\n",
        "        ]\n",
        "        subprocess.run(comando, check=True, capture_output=True)\n",
        "\n",
        "        # Carga el archivo recortado\n",
        "        tasa_muestreo, datos_audio = wavfile.read(archivo_recorte)\n",
        "\n",
        "        # ConversiÃ³n a mono si es necesario\n",
        "        if datos_audio.ndim > 1:\n",
        "            datos_audio = datos_audio.mean(axis=1)\n",
        "        datos_audio = datos_audio / np.max(np.abs(datos_audio))  # NormalizaciÃ³n\n",
        "        seÃ±al_m = datos_audio\n",
        "\n",
        "        # ConstrucciÃ³n del vector de tiempo\n",
        "        tiempo = np.linspace(inicio, inicio + len(seÃ±al_m) / tasa_muestreo, len(seÃ±al_m), endpoint=False)\n",
        "\n",
        "        # Limpieza de archivos temporales\n",
        "        if os.path.exists(archivo_temp):\n",
        "            os.remove(archivo_temp)\n",
        "        if os.path.exists(archivo_recorte):\n",
        "            os.remove(archivo_recorte)\n",
        "\n",
        "        return seÃ±al_m, tasa_muestreo, tiempo\n",
        "\n",
        "    except Exception as e:\n",
        "        st.error(f\"Hubo un error al intentar obtener el audio: {e}\")\n",
        "        if os.path.exists(archivo_temp):\n",
        "            os.remove(archivo_temp)\n",
        "        if os.path.exists(archivo_recorte):\n",
        "            os.remove(archivo_recorte)\n",
        "        return None, None, None\n",
        "\n",
        "def graficar_tiempo(t, seÃ±al, titulo):\n",
        "    \"\"\"Genera un grÃ¡fico temporal de la seÃ±al.\"\"\"\n",
        "    fig, ax = plt.subplots(figsize=(10, 4))\n",
        "    ax.plot(t, seÃ±al, lw=1)\n",
        "    ax.set_title(titulo)\n",
        "    ax.set_xlabel(\"Tiempo (s)\")\n",
        "    ax.set_ylabel(\"Amplitud\")\n",
        "    ax.grid(True)\n",
        "    ax.margins(x=0.01, y=0.1)\n",
        "    plt.tight_layout()\n",
        "    return fig\n",
        "\n",
        "def graficar_frecuencia(seÃ±al, fs, titulo, limite=None):\n",
        "    \"\"\"Genera el espectro de magnitud de la seÃ±al (FFT).\"\"\"\n",
        "    fig, ax = plt.subplots(figsize=(10, 4))\n",
        "    N = len(seÃ±al)\n",
        "    if N == 0: return fig\n",
        "    fft_val = fftshift(fft(seÃ±al))\n",
        "    fft_freqs = fftshift(fftfreq(N, 1 / fs))\n",
        "    ax.plot(fft_freqs, np.abs(fft_val) / N, lw=1)\n",
        "    ax.set_title(titulo)\n",
        "    ax.set_xlabel(\"Frecuencia (Hz)\")\n",
        "    ax.set_ylabel(\"Magnitud\")\n",
        "    ax.grid(True)\n",
        "    if limite:\n",
        "        ax.set_xlim(limite)\n",
        "    plt.tight_layout()\n",
        "    return fig\n",
        "\n",
        "def modular_ssb_fft(m, t, fs, fc, banda):\n",
        "    \"\"\"Modula la seÃ±al m(t) usando SSB mediante filtrado espectral.\"\"\"\n",
        "    seÃ±al_dsb = m * (2 * np.cos(2 * np.pi * fc * t))\n",
        "    espectro_dsb = fftshift(fft(seÃ±al_dsb))\n",
        "    freqs = fftshift(fftfreq(len(t), 1 / fs))\n",
        "\n",
        "    # Filtro para seleccionar banda USB o LSB\n",
        "    if banda == 'USB (Superior)':\n",
        "        filtro = np.where(freqs > fc, 1, 0)\n",
        "    else:\n",
        "        filtro = np.where(freqs < -fc, 1, 0)\n",
        "\n",
        "    espectro_ssb = espectro_dsb * filtro\n",
        "    seÃ±al_ssb = np.real(ifft(ifftshift(espectro_ssb)))\n",
        "    return seÃ±al_ssb\n",
        "\n",
        "def demodular_ssb_fft(ssb, t, fs, fc, bw):\n",
        "    \"\"\"Demodula la seÃ±al SSB aplicando remultiplicaciÃ³n y paso bajo.\"\"\"\n",
        "    seÃ±al_product = ssb * (2 * np.cos(2 * np.pi * fc * t))\n",
        "    espectro_v = fftshift(fft(seÃ±al_product))\n",
        "    freqs = fftshift(fftfreq(len(t), 1 / fs))\n",
        "    paso_bajo = np.where(np.abs(freqs) <= bw * 1.2, 1, 0)\n",
        "    espectro_demod = espectro_v * paso_bajo\n",
        "    mensaje_recuperado = np.real(ifft(ifftshift(espectro_demod)))\n",
        "    return mensaje_recuperado\n",
        "\n",
        "# -----------------------------------------------\n",
        "# INTERFAZ DE USUARIO (barra lateral)\n",
        "# -----------------------------------------------\n",
        "\n",
        "# Encabezado en la barra lateral\n",
        "st.sidebar.header(\"âš™ï¸ Ajustes Generales\")\n",
        "\n",
        "# Tipo de seÃ±al a usar\n",
        "tipo_mensaje = st.sidebar.selectbox(\n",
        "    \"1. Tipo de seÃ±al a modular\",\n",
        "    (\"Pulso Rectangular\", \"Audio desde YouTube\"),\n",
        "    help=\"Selecciona la fuente del mensaje.\"\n",
        ")\n",
        "\n",
        "# InicializaciÃ³n de variables\n",
        "seÃ±al_m, fs, t, bw_mensaje = None, 44100, None, 0\n",
        "duracion = st.sidebar.slider(\"DuraciÃ³n de seÃ±al (s)\", 1, 20, 5, 1)\n",
        "\n",
        "# Caso: Pulso rectangular\n",
        "if tipo_mensaje == \"Pulso Rectangular\":\n",
        "    t = np.linspace(0, duracion, int(duracion * fs), endpoint=False)\n",
        "    ancho_pulso = st.sidebar.slider(\"DuraciÃ³n del pulso (s)\", 0.1, 2.0, 0.5, 0.1)\n",
        "    seÃ±al_m = np.zeros_like(t)\n",
        "    seÃ±al_m[t < ancho_pulso] = 1.0\n",
        "    bw_mensaje = 4 / ancho_pulso\n",
        "    st.sidebar.info(f\"Ancho de banda estimado: {bw_mensaje:.1f} Hz\")\n",
        "\n",
        "# Caso: Audio de YouTube\n",
        "elif tipo_mensaje == \"Audio desde YouTube\":\n",
        "    enlace = st.sidebar.text_input(\"Pega un enlace de YouTube\", \"https://www.youtube.com/watch?v=s5cYXknsDbY\")\n",
        "    inicio = st.sidebar.slider(\"Inicio del audio (s)\", 0, 300, 50, 1)\n",
        "    if enlace:\n",
        "        with st.spinner(f\"Procesando desde el segundo {inicio}...\"):\n",
        "            seÃ±al_m, fs, t = obtener_audio_desde_youtube(enlace, duracion=duracion, inicio=inicio)\n",
        "        bw_mensaje = 20000\n",
        "        if fs:\n",
        "            st.sidebar.info(f\"Ancho de banda: {bw_mensaje} Hz\\nFrecuencia de muestreo: {fs} Hz\")\n",
        "\n",
        "# Frecuencia de portadora\n",
        "fc_maxima = (fs / 2) - bw_mensaje if fs and bw_mensaje > 0 else 20000\n",
        "if fc_maxima < 1000: fc_maxima = 1000\n",
        "fc = st.sidebar.slider(\"Frecuencia de Portadora (Hz)\", 1000.0, fc_maxima, 10000.0, 500.0)\n",
        "\n",
        "# ElecciÃ³n de banda lateral\n",
        "banda = st.sidebar.radio(\"2. Banda lateral a usar\", (\"USB (Superior)\", \"LSB (Inferior)\"))\n",
        "\n",
        "# -----------------------------------------------\n",
        "# PROCESO COMPLETO DE MODULACIÃ“N Y DEMODULACIÃ“N\n",
        "# -----------------------------------------------\n",
        "\n",
        "if seÃ±al_m is not None and t is not None:\n",
        "    # VisualizaciÃ³n de la seÃ±al original\n",
        "    st.header(\"1. SeÃ±al de Entrada m(t)\")\n",
        "    col1, col2 = st.columns(2)\n",
        "    with col1:\n",
        "        st.pyplot(graficar_tiempo(t, seÃ±al_m, \"SeÃ±al Original en Tiempo\"))\n",
        "    with col2:\n",
        "        st.pyplot(graficar_frecuencia(seÃ±al_m, fs, \"Espectro del Mensaje\", limite=(-bw_mensaje * 1.5, bw_mensaje * 1.5)))\n",
        "    if tipo_mensaje == \"Audio desde YouTube\":\n",
        "        st.audio(seÃ±al_m, format=\"audio/wav\", sample_rate=fs)\n",
        "\n",
        "    # Proceso de modulaciÃ³n\n",
        "    st.markdown(\"---\")\n",
        "    st.header(\"2. ModulaciÃ³n en Banda Lateral SSB (FFT)\")\n",
        "    st.write(\"Se construye seÃ±al DSB y se filtra espectralmente para conservar una sola banda.\")\n",
        "    seÃ±al_ssb = modular_ssb_fft(seÃ±al_m, t, fs, fc, banda)\n",
        "    st.pyplot(graficar_frecuencia(seÃ±al_ssb, fs, f\"Espectro Modulado {banda}\", limite=(fc - bw_mensaje * 1.5, fc + bw_mensaje * 1.5)))\n",
        "\n",
        "    # Proceso de demodulaciÃ³n\n",
        "    st.markdown(\"---\")\n",
        "    st.header(\"3. RecuperaciÃ³n de la SeÃ±al Original\")\n",
        "    st.write(\"Se remultiplica con portadora y se filtra para extraer el mensaje original.\")\n",
        "    seÃ±al_recuperada = demodular_ssb_fft(seÃ±al_ssb, t, fs, fc, bw_mensaje)\n",
        "\n",
        "    # Resultados finales\n",
        "    st.write(\"#### Resultado tras DemodulaciÃ³n\")\n",
        "    col1, col2 = st.columns(2)\n",
        "    with col1:\n",
        "        st.pyplot(graficar_tiempo(t, seÃ±al_recuperada, \"SeÃ±al Recuperada en Tiempo\"))\n",
        "    with col2:\n",
        "        st.pyplot(graficar_frecuencia(seÃ±al_recuperada, fs, \"Espectro de la RecuperaciÃ³n\", limite=(-bw_mensaje * 1.5, bw_mensaje * 1.5)))\n",
        "    if tipo_mensaje == \"Audio desde YouTube\":\n",
        "        st.audio(seÃ±al_recuperada, format=\"audio/wav\", sample_rate=fs)\n",
        "\n",
        "else:\n",
        "    st.info(\"Ajusta los controles laterales para comenzar la simulaciÃ³n.\")\n",
        "\n"
